source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '10.0'
project 'marvel-universe'
workspace 'marvel-universe'
inhibit_all_warnings!
use_frameworks!

def import_pods
  pod 'Alamofire', '~> 4.0'
  pod 'Moya', '8.0.0-beta.2'
  pod 'ObjectMapper', '~> 2.0'
  pod 'Swinject', '2.0.0-beta.2'
  pod 'PureLayout', '~> 3.0'
  pod 'R.swift', '~> 3.0'
  pod 'Kingfisher', '~> 3.1'
  pod 'CryptoSwift', '~> 0.6'
end

target :'marvel-universe' do
	import_pods
end
